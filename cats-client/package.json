{
  "name": "cats-workdesk",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@sentry/browser": "^5.6.3",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "percy exec -- wdio wdio.conf.js",
    "predeploy": "npm run build",
    "deploy": "aws s3 sync build/ s3://cats-app --delete",
    "postdeploy": "aws cloudfront create-invalidation --distribution-id E2Q8LPKLN5A9GK --paths '/*'",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@percy/webdriverio": "^1.0.4",
    "@wdio/cli": "^5.13.0",
    "@wdio/dot-reporter": "^5.12.1",
    "@wdio/local-runner": "^5.13.0",
    "@wdio/mocha-framework": "^5.12.1",
    "@wdio/selenium-standalone-service": "^5.12.1"
  }
}
